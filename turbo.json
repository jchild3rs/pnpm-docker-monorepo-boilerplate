{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "build/**"]
    },
    "release": {
      "dependsOn": ["build", "$BRANCH_TAG"],
      "outputs": []
    },
    "check": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": ["$CI", "$TURBO_CACHE_KEY"],
      "outputs": []
    }
  },
  "baseBranch": "origin/main"
}
